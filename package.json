{
  "name": "cypress_learning",
  "version": "1.0.0",
  "description": "",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^18.0.6",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "cypress": "^13.2.0",
    "cypress-iframe": "^1.0.1",
    "cypress-xpath": "^2.0.1",
    "multiple-cucumber-html-reporter": "^3.5.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": [
      "cypress/integration/**/*.js"
    ],
    "json": {
      "enabled": true,
      "output": "cypress/reports/json/cucumber-report.json"
    },
    "messages": {
      "enabled": true,
      "output": "cypress/reports/messages/cucumber-report.ndjson"
    }
  },
  "scripts": {
    "open": "cypress open",
    "headless": "cypress run --browser chrome --reporter json",
    "test": "cypress run --headed --browser chrome --reporter json --env configFile=dev",
    "report": "node html-report.js"
  }
}
